#!/usr/bin/env python3

import os, sys
import argparse

parser = argparse.ArgumentParser()
add = parser.add_argument

add('command')

if __name__ == '__main__':
	args = parser.parse_args()
	cmd = args.command
	for line in sys.stdin.read().split('\n'):
		os.system('{} "{}"'.format(cmd, '" "'.join(line.split('|'))))
